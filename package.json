{
  "name": "directus-ssr-app",
  "version": "1.0.0",
  "packageManager": "pnpm@8.6.7",
  "description": "",
  "author": "",
  "license": "ISC",
  "keywords": [],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:spa": "vite dev --port 1337",
    "dev": "SSR_ENV=development npx directus start",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/main.ts",
    "start": "npx directus start",
    "apply:snapshot": "npx directus schema apply --yes ./_snapshot/snapshot.yaml"
  },
  "dependencies": {
    "@directus/sdk": "^12.0.0",
    "@unocss/reset": "^0.56.1",
    "@vueuse/core": "^10.3.0",
    "@vueuse/head": "^2.0.0",
    "@vueuse/router": "^10.3.0",
    "directus": "^10.6.2",
    "directus-extension-ssr": "^2.0.8",
    "pinia": "^2.1.6",
    "sqlite3": "^5.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@unocss/eslint-config": "^0.56.1",
    "@vitejs/plugin-vue": "^4.3.4",
    "eslint": "^8.50.0",
    "typescript": "^5.2.2",
    "unocss": "^0.56.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.4.9",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-webfont-dl": "^3.7.6"
  }
}
